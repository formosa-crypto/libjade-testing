require "../src/dilithium3/sign.jazz"
require "../src/dilithium3/variant_params.jazz"

export
fn compute_mu_jazz(reg u64 ptr_m, reg u64 m_len, reg u64 ptr_sk, reg u64 ptr_mu)
{
	stack u8[Li2_SK_LEN] sk;
	stack u8[64] mu;
	inline int i;
	reg u8 c;
	
	for i = 0 to Li2_SK_LEN {
		c = (u8) [ptr_sk + i];
		sk[i] = c;
	}
	mu = compute_mu(ptr_m, m_len, sk);

	for i = 0 to 64 {
		c = mu[i];
		(u8) [ptr_mu + i] = c;
	}
}
