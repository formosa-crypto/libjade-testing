require "../src/common/expandMask.jazz"
require "../src/common/packing.jazz"

// TODO: Simplify with preamble/epilogue subroutines

export
fn expandMask_poly_gamma1_219_jazz(reg u64 rho_prime_ptr, reg u16 nonce, reg u64 ptr_y) {
	stack u8[64] rho_prime;
	stack u32[Li2_polydeg] y_elem;

	reg u64 i;
	reg u8 c;
	reg u32 v;

	i = #set0_64();
	while (i < 64) {
		c = (u8)[rho_prime_ptr + i];
		rho_prime[(int) i] = c;

		i += 1;
	}

	y_elem = expandMask_poly_gamma1_219(y_elem, rho_prime, nonce);

	i = #set0_64();
	while (i < Li2_polydeg) {
		v = y_elem[(int) i];
		(u32) [ptr_y + 4 * i] = v;

		i += 1;
	}
}

