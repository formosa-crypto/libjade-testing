export
fn poly_pointwise_montgomery_jazz(reg u64 f_ptr, reg u64 g_ptr, reg u64 prod_ptr) {
	stack u32[Li2_polydeg] f g prod;

	reg u32 v;
	reg u64 i;

	?{}, i = #set0_64();
	while (i < Li2_polydeg) {
		v = (u32) [f_ptr + 4 * i];
		f[(int) i] = v;
		v = (u32) [g_ptr + 4 * i];
		g[(int) i] = v;
		i += 1;
	}

	prod = poly_pointwise_montgomery(f, g, prod);

	?{}, i = #set0_64();
	while (i < Li2_polydeg) {
		v = prod[(int) i];
		(u32) [prod_ptr + 4 * i] = v;
		i += 1;
	}
}
