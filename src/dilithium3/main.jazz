require "expandA.jazz"
require "expandS.jazz"
require "expandMask.jazz"
require "polyvec.jazz"
require "packing.jazz"
require "hashing.jazz"
require "params.jazz"

require "../common/keygen_end.jazz"
require "../common/sign_end.jazz"
require "../common/verify_end.jazz"

export
fn keygen_jazz(reg u64 pk_ptr, reg u64 sk_ptr, reg u64 randomness) {
	keygen(pk_ptr, sk_ptr, randomness);
}

export
fn sign_jazz(reg u64 ptr_signature, reg u64 ptr_m, reg u64 m_len, reg u64 ptr_sk) {
	sign(ptr_signature, ptr_m, m_len, ptr_sk);
}

export
fn verify_jazz(reg u64 ptr_sig, reg u64 ptr_m, reg u64 m_len, reg u64 ptr_pk) -> reg u32 {
    reg u32 status;
    status = verify(ptr_sig, ptr_m, m_len, ptr_pk);
    return status;
}
